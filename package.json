{
  "name": "light-countdown",
  "version": "1.0.4",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/plasusu/countdown.git"
  },
  "keywords": [
    "countdown",
    "javascript"
  ],
  "description": "countdown plugin",
  "main": "index.js",
  "jsnext:main": "src/index.js",
  "author": "plasusu",
  "license": "MIT",
  "scripts": {
    "env": "npm install && npm update",
    "build": "node build.js",
    "dev": "webpack --progress --colors --watch",
    "server": "http-server -p 80",
    "example": "webpack --config ./example/webpack.ui.config.js --progress --colors --watch",
    "test": "node_modules/.bin/ava test/*.js",
    "test:coverage": "node_modules/.bin/nyc npm run test",
    "test:report": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls"
  },
  "dependencies": {},
  "devDependencies": {
    "ava": "^0.24.0",
    "babel-core": "6.24.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "7.1.2",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-istanbul": "4.1.5",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.24.0",
    "babel-runtime": "6.9.2",
    "browser-env": "3.2.4",
    "coveralls": "^3.0.0",
    "css-loader": "0.28.0",
    "http-server": "0.9.0",
    "json-loader": "0.5.4",
    "less": "^2.7.2",
    "less-loader": "4.0.3",
    "nyc": "^11.4.1",
    "postcss-loader": "^2.0.10",
    "rewire": "^3.0.2",
    "rimraf": "2.5.4",
    "style-loader": "0.16.1",
    "vconsole": "^2.5.2",
    "webpack": "^3.10.0",
    "xtemplate": "4.6.0"
  },
  "files": [
    "lib",
    "src",
    "index.js"
  ],
  "ava": {
    "require": [
      "babel-register",
      "./test/helpers/setup.js"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 10,
    "extension": [
      ".js"
    ],
    "exclude": [
      "*/config/*",
      "src/transport/index.js",
      "test/*"
    ],
    "reporter": [
      "html",
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
